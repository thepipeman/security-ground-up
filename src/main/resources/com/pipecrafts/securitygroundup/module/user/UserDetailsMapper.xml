<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.pipecrafts.securitygroundup.module.user.UserDetailsMapper">

  <select id="selectByUserName" resultType="UserDetails">
    SELECT *
    FROM user_details
    WHERE username = #{username}
  </select>
</mapper>